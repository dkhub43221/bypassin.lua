local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Camera = workspace.CurrentCamera
local LocalPlayer = Players.LocalPlayer

local selectedPlayerName
local loopTpConnection

local function getPlayerNames()
    local names = {}
    for _, player in ipairs(Players:GetPlayers()) do
        table.insert(names, player.Name)
    end
    return names
end

local function getTarget()
    if not selectedPlayerName then return end
    local plr = Players:FindFirstChild(selectedPlayerName)
    if not plr or not plr.Character then return end
    return plr,
        plr.Character:FindFirstChild("HumanoidRootPart"),
        plr.Character:FindFirstChildOfClass("Humanoid")
end

local playerDropdown
playerDropdown = MSection:Dropdown({
    Name = "Select Player",
    Flag = "player_select",
    MaxSize = 145,
    Items = getPlayerNames(),
    Callback = function(option)
        selectedPlayerName = option
    end
})

local function refreshDropdown()
    local items = getPlayerNames()
    if playerDropdown.Set then
        playerDropdown:Set(items)
    elseif playerDropdown.Refresh then
        playerDropdown:Refresh(items)
    elseif playerDropdown.Update then
        playerDropdown:Update(items)
    end
end

Players.PlayerAdded:Connect(refreshDropdown)
Players.PlayerRemoving:Connect(refreshDropdown)

MSection:Button({
    Name = "Teleport To Selected Player",
    Callback = function()
        local _, hrp = getTarget()
        if hrp then
            tp(hrp.Position)
        end
    end
})

MSection:Toggle({
    Name = "Spectate Selected Player",
    Flag = "spectate_player",
    Default = false,
    Callback = function(Value)
        local _, _, humanoid = getTarget()
        if Value and humanoid then
            Camera.CameraSubject = humanoid
        else
            local myHumanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            if myHumanoid then
                Camera.CameraSubject = myHumanoid
            end
        end
    end
})

MSection:Toggle({
    Name = "Bring Selected Player",
    Flag = "bring_player",
    Default = false,
    Callback = function(Value)
        if not Value then return end
        local _, targetHrp = getTarget()
        local myHrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if targetHrp and myHrp then
            targetHrp.CFrame = myHrp.CFrame
        end
    end
})

MSection:Toggle({
    Name = "Loop Teleport To Selected Player",
    Flag = "loop_tp_player",
    Default = false,
    Callback = function(Value)
        if Value then
            loopTpConnection = RunService.Heartbeat:Connect(function()
                local _, hrp = getTarget()
                if hrp then
                    tp(hrp.Position)
                end
            end)
        else
            if loopTpConnection then
                loopTpConnection:Disconnect()
                loopTpConnection = nil
            end
        end
    end
})

