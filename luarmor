local Remote = ReplicatedStorage:WaitForChild("yunter")

local tweetText = "Bypassin On top - discord.gg/bypassin"
local spamming = false

TrollSection:Textbox({
    Name = "Tweet Messages",
    Default = "",
    Placeholder = "Input",
    Callback = function(Text)
        tweetText = Text
    end
})

TrollSection:Button({
    Name = "Send Tweet",
    Callback = function()
        local args = {
            tweetText,
            "\226\128\142 Pr\226\128\142 1V\226\128\142 4t3\226\128\142 "
        }
        Remote:FireServer(unpack(args))
    end
})

TrollSection:Toggle({
    Name = "Spam Tweet",
    Default = false,
    Callback = function(Value)
        spamming = Value
        if spamming then
            task.spawn(function()
                while spamming do
                    local args = {
                        tweetText,
                        "\226\128\142 Pr\226\128\142 1V\226\128\142 4t3\226\128\142 "
                    }
                    Remote:FireServer(unpack(args))
                    task.wait(3)
                end
            end)
        end
    end
})
