    Main = Color3.fromRGB(20, 20, 20),
    Accent = Color3.fromRGB(255, 0, 0),
    Text = Color3.fromRGB(255, 255, 255)
}

for Index, Color in pairs(CustomTheme) do
    Library.Theme[Index] = Color
    Library:ChangeTheme(Index, Color)
end

